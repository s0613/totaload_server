Content-Type: application/json

{
  "email": "incomplete_eval2025@example.com",
  "username": "incomplete_evaluator2025",
  "password": "evalpass123",
  "confirmPassword": "evalpass123",
  "name": "미완성 평가사",
  "isEvaluator": true,
  "evaluatorCertNumber": "EVAL-2025-00002"
}

